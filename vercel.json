{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "m/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/proxy",
      "dest": "api/proxy.js"
    },
    {
      "src": "/m/(.*)",
      "dest": "/m/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
